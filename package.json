{
  "name": "playwright_testautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cross-env ENV_Test=uat npx playwright test --project=chrome --headed --debug",
    "test:company": "cross-env ENV_Test=sit npx playwright test --grep @smoke --project=chrome --headed",
    "test:company:lambdatest": "cross-env ENV_Test=sit npx playwright test --grep @company --project=chrome@lambdatest --headed",
    "uat1test": "cross-env ENV_Test=uat npx playwright test --project=chrome --headed",
    "demotest": "cross-env ENV_Test=sit npx playwright test --grep @company",
    "demosittestwithchromium": "cross-env ENV_Test=sit npx playwright test --grep @company --project=chromium --headed",
    "post----test": "npx playwright show-report",
    "dot:report": "npx playwright test --reporter=dot",
    "line:report": "npx playwright test --reporter=line",
    "allure-generate": "allure generate ./allure-results --clean -o allure-report && allure open allure-report",
    "allure:report": "npm run allure-generate",
    "post---test": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "test-1": "npx playwright test create_company.spec.js --reporter=line,allure-playwright",
    "allureClear": "if exist allure-results rmdir /q /s allure-results && mkdir allure-results && if exist allure-report rmdir /q /s allure-report && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos"
  },
  "keywords": [
    "Plawright",
    "Test Automation",
    "JavaScript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.38.1",
    "@types/node": "^20.8.0",
    "allure-commandline": "^2.24.0",
    "allure-playwright": "^2.8.1",
    "cross-env": "^7.0.3",
    "ortoni-report": "^2.0.1"
  },
  "dependencies": {
    "@lambdatest/node-rest-client": "^2.0.1",
    "dotenv": "^16.4.5",
    "playwright": "^1.42.1",
    "xlsx": "^0.18.5"
  }
}
